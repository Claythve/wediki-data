<md>
# Home
## Homework
$T$Homework
</md>
<js>
request.async.get.json('https://eu-claythve.herokuapp.com/api/subjects').then(subjects => {
    let data = [];

    for (let subject of subjects) {
        let d = {};
        d.subject = subject.name;
        d.homework = subject.homework.map(x => x.description).join(",");
        data.push(d);
    }
    tables.push({
        title: "Homework",
        columns: ["subject", "homework"],
        data: Array.from(data)
    });
});
</js>
