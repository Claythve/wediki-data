<js>
console.log(tables, "BEFORE");
tables = [];

const resp = await fetch("https://eu-claythve.herokuapp.com/api/subjects");
const subjects = await resp.json();
let data = [];
console.log(subjects);
for (subject of subjects)
{
  let d = { };
  d.subject = subject.name;
  d.homework = subject.homework.map(x => x.description).join(",");
  data.push(data);
}
tables.push({
  title: "Homework",
  columns: ["subject", "homework"],
  data: data
});
console.log(tables);
</js>
