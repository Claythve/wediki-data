<md>
# Home
## Homework
$T$Homework
</md>
<js>
tables = [];

const subjects = getRequest.json('https://eu-claythve.herokuapp.com/api/subjects');
let data = [];
for (let subject of subjects)
{
  let d = { };
  d.subject = subject.name;
  d.homework = subject.homework.map(x => x.description).join(",");
  data.push(d);
}
tables = [{
  title: "Homework",
  columns: ["subject", "homework"],
  data: Array.from(data)
}];
</js>
